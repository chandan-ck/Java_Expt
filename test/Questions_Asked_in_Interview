---Check the randomness and how to ensure the probability of getting the element is same---
Class CustumSet{
    
    public static final Object instance=new Object();
    
    
     Map<Integer,Object> Entry;
    CustumSet(){
       entry=new HashMap<Integer,Object>;
        }
    
    
    public boolean insert(Integer i){
        int randomIndex= (int)(size * Math.randam());
        
        
        if(entry.put(i,randomIndex)==null)
            return true;
        else
            return false;       
        
        }
        
        public boolean remove(Integer i){
        
        if(entry.containsKey(i))
               entry.remove(i);
        else
            return false;
        }
        
       public  int getRandom(){
           
           int size=entry.size();
           
           if(size==0)
               sop("no element present);
           
           int randomIndex= (int)(size * Math.randam());
           
           return 
           }
        
    }

